find . -type f -exec bash -c 'echo this is file path $0' {} \;
find /opt -type f -newer /bin/bash -exec basename {} \;
find . -type f -size -3k -exec bash -c ' echo "Adding more line" >> "$1"' _ {} \;-- right
find . -type f -size +1c -exec  basename {} \;---- right bytes data
 find . -type f -size -1k -exec cat "$1" _ {} \;

